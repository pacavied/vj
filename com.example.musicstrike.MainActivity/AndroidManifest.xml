<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.musicstrike"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <application
        
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gameClassicActivity"
            android:label="@string/title_activity_game_classic" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.musicstrike.MainActivity" />
        </activity>
        <activity
            android:name=".loseWindowActivity"
            android:label="@string/title_activity_game_classic" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.musicstrike.MainActivity" />
        </activity>
        <activity
            android:name=".InitialCountDownActivity"
            android:label="@string/title_activity_game_classic" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.musicstrike.MainActivity" />
        </activity>
        <activity
            android:name=".TutorialActivity"
            android:label="@string/title_activity_game_classic" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.musicstrike.MainActivity" />
        </activity>
        <activity
            android:name=".TutorialScreens"
            android:label="@string/title_activity_game_classic" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.musicstrike.MainActivity" />
        </activity>
    </application>

</manifest>